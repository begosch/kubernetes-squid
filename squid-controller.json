{
  "kind":"ReplicationController",
  "apiVersion":"v1",
  "metadata":{
    "name":"squid"
  },
  "spec":{
    "replicas":1,
    "selector":{
      "component":"squid"
    },
    "template":{
      "metadata":{
        "labels":{
          "component":"squid"
        }
      },
      "spec":{
        "containers":[
        {
          "name":"squid",
          "image":"jpetazzo/squid-in-a-can",
          "ports":[
          {
            "name":"squid",
            "containerPort":3129
          }
          ]
        }
        ]
      }
    }
  }
}
